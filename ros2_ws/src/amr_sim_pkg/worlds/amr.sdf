        <!-- Rectangular Pillars -->
        <model name='box_obstacle_1'>
            <static>true</static>
            <pose>-5 -5 0.5 0 0 0</pose>
            <link name='box'>
                <visual name='visual'>
                    <geometry>
                        <box>
                            <size>1.0 1.0 1.0</size>
                        </box>
                    </geometry>
                    <material>
                        <ambient>1.0 0.0 0.0 1</ambient>
                        <diffuse>1.0 0.0 0.0 1</diffuse>
                        <specular>1.0 0.0 0.0 1</specular>
