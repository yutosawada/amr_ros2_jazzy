        </model>

        <!-- Internal Walls -->
        <model name='internal_wall_1'>
            <static>true</static>
            <pose>0 6 0.5 0 0 0.785</pose>
            <link name='box'>
                <visual name='visual'>
                    <geometry>
                        <box>
                            <size>0.2 4.0 1.0</size>
                        </box>
                    </geometry>
                    <material>
                        <ambient>0.0 0.0 1.0 1</ambient>
                        <diffuse>0.0 0.0 1.0 1</diffuse>
                        <specular>0.0 0.0 1.0 1</specular>
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <box>
                            <size>0.2 4.0 1.0</size>
                        </box>
                    </geometry>
                </collision>
            </link>
        </model>

        <model name='internal_wall_2'>
            <static>true</static>
            <pose>6 0 0.5 0 0 0</pose>
            <link name='box'>
                <visual name='visual'>
                    <geometry>
                        <box>
                            <size>0.2 4.0 1.0</size>
                        </box>
                    </geometry>
                    <material>
                        <ambient>0.0 0.0 1.0 1</ambient>
                        <diffuse>0.0 0.0 1.0 1</diffuse>
